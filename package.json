{
  "name": "detect-it-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://detect-it.rafgraph.dev",
  "dependencies": {
    "@stitches/react": "0.0.2",
    "detect-it": "^4.0.0-next.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-interactive": "^0.9.2",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.68",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "browserslist-config-css-grid": "^1.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.1",
    "prettier": "^2.1.2",
    "react-scripts": "4.0.0-next.98",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages --dist build --message \"Built from main branch `date +%Y%m%d`-`date +%H%M%S`\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 versions or > 0.2% and not dead and extends browserslist-config-css-grid"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown"
  }
}
